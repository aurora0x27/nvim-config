  "acm": {
    "prefix": ["acm"],
    "body": [
      "#include <bits/stdc++.h>",
      "",
      "using namespace std;",
      "using i64 = long long;",
      "using u64 = unsigned long long;",
      "using f64 = double;",
      "constexpr i64 INF = 1e18;",
      "constexpr f64 EPS = 1e-6;",
      "",
      "template <typename... Ts>",
      "__attribute__((always_inline)) void read(Ts &...args) {",
      "\t(std::cin >> ... >> args);",
      "}",
      "",
      "template <typename... Ts>",
      "__attribute__((always_inline)) auto read() {",
      "\tstd::tuple<Ts...> t;",
      "\tstd::apply([](auto &...x) { (std::cin >> ... >> x); }, t);",
      "\treturn t;",
      "}",
      "",
      "void solv() {",
      "\t${0}",
      "}",
      "",
      "int main(void) {",
      "\tios::sync_with_stdio(false);",
      "\tcin.tie(nullptr);",
      "\tunsigned N;",
      "\tread(N);",
      "\twhile (N--) {",
      "\t\tsolv();",
      "\t}",
      "\treturn 0;",
      "}"
    ],
    "description": "ACM style template"
  },
