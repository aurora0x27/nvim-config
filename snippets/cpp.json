    "description": "enum def"
  },
  "for": {
    "prefix": "for",
    "body": ["for (${1:init}; ${2:conf}; ${3:post}) {", "${0}", "}"],
    "description": "for loop"
  },
  "for with range": {
    "prefix": "forr",
    "body": ["for (auto &{1:item} : ${2:iterated}) {", "}"],
    "description": "for loop with range"
  },
  "do": {
    "prefix": "do",
    "body": ["do {", "\t${0}", "} while (${1:0});"],
    "description": "while loop"
  },
  "while": {
    "prefix": "while",
    "body": ["while (${1:1}) {", "\t${0}", "}"],
    "description": "while loop"
  },
  "switch": {
    "prefix": "switch",
    "body": ["switch (${1:cond}) {", "${0}", "default: {}", "}"],
    "description": "for loop"
  },
  "#if": {
    "prefix": "#if",
    "body": ["#if ${1:0}", "$0", "#endif /* if $1 */"],
    "description": "#if snippet"
  },
  "#ifdef": {
    "prefix": "#ifdef",
    "body": ["#ifdef ${1:MACRO}", "$0", "#endif /* ifdef $1 */"],
    "description": "#ifdef snippet"
  },
  "#ifndef": {
    "prefix": "#ifndef",
    "body": ["#ifndef ${1:MACRO}", "$0", "#endif /* ifndef $1 */"],
    "description": "#ifndef snippet"
  },
  "include once": {
    "prefix": ["#once", "guard", "#guard"],
    "body": ["#ifndef ${1:FILE}_H", "#define $1_H", "$0", "", "#endif // $1_H"],
